{
    "name": "Node-RED Proxy",
    "version": "0.0.1",
    "slug": "nodered_proxy",
    "description": "Proxy for externally running Node-RED",
    "url": "https://github.com/Ericsson2506/addon-test",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:resistor-nodes",
    "map": [
      "config:ro"
    ],
    "options": {
      "server": null
    },
    "schema": {
      "server": "http://xxx.xxx.xxx.xxx:1880/"
    },
    "image": "zigbee2mqtt/zigbee2mqtt-proxy-{arch}"
}
