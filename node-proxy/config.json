{
    "name": "Node-RED Proxy",
    "version": "0.2.0",
    "slug": "nodered_proxy",
    "description": "Proxy for externally running Node-RED",
    "url": "https://github.com/node-red/node-red",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:nodejs",
    "map": [
      "config:ro"
    ],
    "ports": {
      "1880/tcp": null
    },
    "ports_description": {
      "1880/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null,
      "auth_token": ""
    },
    "schema": {
      "server": "url",
      "auth_token": "str?"
    },
    "image": "nodered/nodered-proxy-{arch}"
}
